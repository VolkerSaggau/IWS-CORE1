

view F_CUS_CNTC_EVT__D_HOUR__REF_72839 (
   FACT,
   DIMENSION,
   DIRECTION,
   KEY,
   COUNTER,
   SOURCE,
   INTEGRITYSTATE
)
AS
(
   select 
      'F_CUS_CNTC_EVT'                         AS FACT,
      'D_HOUR'                                 AS DIMENSION,
      'Inbound'                                AS DIRECTION,
   	to_varchar(FACT.HOUR_ID)                 AS KEY,
   	count(*)                                 AS COUNTER,
      'in: ' || 'D_HOUR'                       AS SOURCE,
      'info' AS INTEGRITYSTATE
   from 
   	F_CUS_CNTC_EVT	 FACT
   WHERE 
      FACT.HOUR_ID IN (select DIMENSION.HOUR_ID from D_HOUR	DIMENSION)
   GROUP BY 
      FACT.HOUR_ID
   
   --
   --

UNION ALL

   select 
      'F_CUS_CNTC_EVT'                         AS FACT,
      'D_HOUR'                                 AS DIMENSION,
      'Inbound'                                AS DIRECTION,
   	to_varchar(FACT.HOUR_ID)                 AS KEY,
   	count(*)                                 AS COUNTER,
      'in: ' || 'D_HOUR'                       AS SOURCE,
      'error' AS INTEGRITYSTATE
   from 
   	F_CUS_CNTC_EVT	 FACT
   WHERE 
      FACT.HOUR_ID NOT IN (select DIMENSION.HOUR_ID from D_HOUR	DIMENSION)
   GROUP BY 
      FACT.HOUR_ID
   
   --
   --

)
ORDER BY 
   FACT DESC, 
   DIMENSION DESC, 
   INTEGRITYSTATE DESC, 
   SOURCE DESC, 
   COUNTER DESC, 
   KEY DESC
;

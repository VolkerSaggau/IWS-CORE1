

view A_CORE_EVT_SUMM__D_GEOGRAPHY__REF_252012 (
   FACT,
   DIMENSION,
   DIRECTION,
   KEY,
   COUNTER,
   SOURCE,
   INTEGRITYSTATE
)
AS
(
   select 
      'A_CORE_EVT_SUMM'                        AS FACT,
      'D_GEOGRAPHY'                            AS DIMENSION,
      'Inbound'                                AS DIRECTION,
   	to_varchar(FACT.GEO_ID)                  AS KEY,
   	count(*)                                 AS COUNTER,
      'in: ' || 'D_GEOGRAPHY'                  AS SOURCE,
      'info' AS INTEGRITYSTATE
   from 
   	A_CORE_EVT_SUMM	 FACT
   WHERE 
      FACT.GEO_ID IN (select DIMENSION.GEO_ID from D_GEOGRAPHY	DIMENSION)
   GROUP BY 
      FACT.GEO_ID
   
   --
   --

UNION ALL

   select 
      'A_CORE_EVT_SUMM'                        AS FACT,
      'D_GEOGRAPHY'                            AS DIMENSION,
      'Inbound'                                AS DIRECTION,
   	to_varchar(FACT.GEO_ID)                  AS KEY,
   	count(*)                                 AS COUNTER,
      'in: ' || 'D_GEOGRAPHY'                  AS SOURCE,
      'error' AS INTEGRITYSTATE
   from 
   	A_CORE_EVT_SUMM	 FACT
   WHERE 
      FACT.GEO_ID NOT IN (select DIMENSION.GEO_ID from D_GEOGRAPHY	DIMENSION)
   GROUP BY 
      FACT.GEO_ID
   
   --
   --

)
ORDER BY 
   FACT DESC, 
   DIMENSION DESC, 
   INTEGRITYSTATE DESC, 
   SOURCE DESC, 
   COUNTER DESC, 
   KEY DESC
;

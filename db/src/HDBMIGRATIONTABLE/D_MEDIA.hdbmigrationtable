== version = 1



column table D_MEDIA (
   MEDIA_ID  INTEGER  NOT NULL   comment 'Media Type ID',
   MEDIA_SHRT_NAM  NVARCHAR(15)     comment 'Media Type Short Name',
   MEDIA_LONG_NAM  NVARCHAR(30)     comment 'Media Type Long Name',
   M_ALL_ENTRIES_IND  NVARCHAR(1)     comment 'All Entries Indicator'
, primary key  ( 
     MEDIA_ID)
)

comment 'Media'


-- end of D_MEDIA
--
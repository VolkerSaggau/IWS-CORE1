== version = 1



column table "D_ACCOUNT" (
   "ACCOUNT_ID"  INTEGER  NOT NULL   comment 'Account ID',
   "ACCT_BEGIN_DATE"  TIMESTAMP,
   "ACCT_END_DATE"  TIMESTAMP     comment 'End Date Period ID',
   "ACCOUNT_NUMBER"  NVARCHAR(18),
   "ACCOUNT_DESC"  NVARCHAR(255),
   "DEROG_CODE"  NVARCHAR(3),
   "ACCT_ALL_ENTRIES_I"  NVARCHAR(1)     comment 'All Entries Indicator'
, primary key  ( 
     "ACCOUNT_ID")
)

comment 'Account'


-- end of D_ACCOUNT

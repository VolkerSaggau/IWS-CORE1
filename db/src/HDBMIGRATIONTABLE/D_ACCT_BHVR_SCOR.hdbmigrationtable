== version = 1



column table "D_ACCT_BHVR_SCOR" (
   "BEHAVIOR_SCORE_ID"  INTEGER  NOT NULL   comment 'Behavior Score',
   "BHVR_SCORE_BAND"  NVARCHAR(50),
   "BHVR_SCOR_DESC"  NVARCHAR(35)  NOT NULL   comment 'Behavior Score Band Description',
   "BABS_ALL_ENTR_IND"  NVARCHAR(1)     comment 'All Entries Indicator'
, primary key  ( 
     "BEHAVIOR_SCORE_ID")
)

comment 'Account Behavior Score'


-- end of D_ACCT_BHVR_SCOR

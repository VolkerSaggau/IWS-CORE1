== version = 1



column table "D_MSR_UNIT" (
   "MEASURE_UNIT_ID"  INTEGER  NOT NULL   comment 'Measurement Unit ID',
   "SHRT_DESC"  NVARCHAR(15)     comment 'Measurement Unit Short Description',
   "LONG_DESC"  NVARCHAR(35),
   "MU_ALL_ENTR_IND"  NVARCHAR(1)     comment 'All Entries Indicator'
, primary key  ( 
     "MEASURE_UNIT_ID")
)

comment 'Measure Unit'


-- end of D_MSR_UNIT

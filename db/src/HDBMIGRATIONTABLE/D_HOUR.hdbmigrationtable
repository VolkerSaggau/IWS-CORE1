== version = 1



column table "D_HOUR" (
   "HOUR_ID"  INTEGER  NOT NULL   comment 'Hour ID',
   "HOUR_NUMBER"  DECIMAL     comment 'Hour Number',
   "AMPM_FLAG"  SMALLINT     comment 'AMPM FLAG',
   "HOUR_RANGE"  NVARCHAR(50)     comment 'HOUR RANGE',
   "MINUTE_IN_HOUR"  INTEGER,
   "MINUTE_RANGE"  NVARCHAR(50),
   "H_ALL_ENTRIES_IND"  NVARCHAR(1)     comment 'All Entries Indicator'
, primary key  ( 
     "HOUR_ID")
)

comment 'Hour'


-- end of D_HOUR
--
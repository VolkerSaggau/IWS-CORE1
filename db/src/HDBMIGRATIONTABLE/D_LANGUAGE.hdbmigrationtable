== version = 1



column table "D_LANGUAGE" (
   "LANGUAGE_ID"  INTEGER  NOT NULL   comment 'Language ID',
   "ISO_LANG_CODE"  NVARCHAR(3)     comment 'ISO Language Code',
   "ISO_LANG_NAME"  NVARCHAR(30)     comment 'ISO Language Name',
   "LANG_GROUP"  NVARCHAR(30)     comment 'LANGUAGE GROUP',
   "LNG_ALL_ENTRIES_IN"  NVARCHAR(1)     comment 'All Entries Indicator'
, primary key  ( 
     "LANGUAGE_ID")
)

comment 'Language'


-- end of D_LANGUAGE
--
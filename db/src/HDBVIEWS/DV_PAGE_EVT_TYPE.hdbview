

view DV_PAGE_EVT_TYPE comment 'Page Event Type'(
PAGE_EVT_TYPE_ID,
EVT_TYPE_NAME,
EVT_TYPE_DESC,
EVT_SUBTYPE_NAME
)
as
select 
    EVT_TYP_ID PAGE_EVT_TYPE_ID, EVT_TYP_SHRT_NAM EVT_TYPE_NAME, 
EVT_TYP_LONG_NAM EVT_TYPE_DESC, EVT_TYP_SUBTYP_NAM EVT_SUBTYPE_NAME
from 
    D_CORE_EVT_TYP
where 
    D_CORE_EVT_TYP.EVT_TYP_CAT_SNM = 'Page'


----
---- End of DV_PAGE_EVT_TYPE

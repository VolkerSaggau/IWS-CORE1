

view DV_SALE_EVT_TYP comment 'Sales Event Type'(
SALE_EVT_TYP_ID,
SALE_EVT_TYP_SNM,
SALE_EVT_TYP_LNM,
SALE_EVT_SUBTYP_NM
)
as
select 
    EVT_TYP_ID  SALE_EVT_TYP_ID, 
    EVT_TYP_SHRT_NAM SALE_EVT_TYP_SNM, 
EVT_TYP_LONG_NAM SALE_EVT_TYP_LNM, EVT_TYP_SUBTYP_NAM SALE_EVT_SUBTYP_NM
from 
    D_CORE_EVT_TYP
where 
    D_CORE_EVT_TYP.EVT_TYP_CAT_SNM = 'Sales'



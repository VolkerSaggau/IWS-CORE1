

view DV_SBRD_ACCOUNT comment 'Subordinate Account'(
SBRD_ACCT_ID,
SBRD_ACCT_BEGIN_DT,
SBRD_ACCT_END_DT,
SBRD_ACCT_NUMBER,
SBRD_ACCT_DESC,
SBRD_DEROG_CODE
)
as
select 
    ACCOUNT_ID      SBRD_ACCT_ID, 
    ACCT_BEGIN_DATE SBRD_ACCT_BEGIN_DT, 
    ACCT_END_DATE   SBRD_ACCT_END_DT, 
    ACCOUNT_NUMBER  SBRD_ACCT_NUMBER, 
    ACCOUNT_DESC    SBRD_ACCT_DESC, 
    DEROG_CODE      SBRD_DEROG_CODE
from 
    D_ACCOUNT


----
---- End of DV_SBRD_ACCOUNT

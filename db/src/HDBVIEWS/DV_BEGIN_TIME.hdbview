

view DV_BEGIN_TIME comment 'Begin Time'(
BEGIN_TIME_ID,
BTS_HOUR_NUMBER,
BTS_AMPM_FLAG,
BTS_HOUR_RANGE,
BTS_MINUTE_IN_HOUR,
BTS_MINUTE_RANGE,
BTS_ALL_ENTR_IND
)
as
select 
    HOUR_ID BEGIN_TIME_ID, HOUR_NUMBER BTS_HOUR_NUMBER, AMPM_FLAG BTS_AMPM_FLAG, 
HOUR_RANGE BTS_HOUR_RANGE, MINUTE_IN_HOUR BTS_MINUTE_IN_HOUR, 
MINUTE_RANGE BTS_MINUTE_RANGE, H_ALL_ENTRIES_IND BTS_ALL_ENTR_IND
from 
    D_HOUR



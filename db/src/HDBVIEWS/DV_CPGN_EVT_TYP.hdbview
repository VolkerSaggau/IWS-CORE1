

view DV_CPGN_EVT_TYP comment 'Campaign Communication Event Type'(
CPGN_EVT_TYP_ID,
CPGN_EVT_TYP_SNM,
CPGN_EVT_TYP_LNM,
CPGN_EVT_SUBTYP_NM
)
as
select 
    EVT_TYP_ID CPGN_EVT_TYP_ID, EVT_TYP_SHRT_NAM CPGN_EVT_TYP_SNM, 
EVT_TYP_LONG_NAM CPGN_EVT_TYP_LNM, EVT_TYP_SUBTYP_NAM CPGN_EVT_SUBTYP_NM
from 
    D_CORE_EVT_TYP
where 
    EVT_TYP_CAT_SNM = 'Campaign'


----
---- End of DV_CPGN_EVT_TYP

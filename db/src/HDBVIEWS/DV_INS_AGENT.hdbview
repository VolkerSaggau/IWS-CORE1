

view "DV_INS_AGENT" comment 'Insurance Agent'(
"INS_AGENT_ID",
"AGENT_SUBCAT_NAME",
"AGENT_NBR",
"LAG_FIRST_NAME",
"LAG_MIDDLE_NAME",
"LAG_LAST_NAME",
"LAG_TAX_ID",
"LAG_PRIM_TEL_NBR",
"LAG_SCND_TEL_NBR",
"LAG_FAX_NBR",
"LAG_EMAIL"
)
as
select 
    ENTY_ID                 INS_AGENT_ID, 
    ENTY_SUBCAT_NAME        AGENT_SUBCAT_NAME, 
ENTY_UNIQUE_ID AGENT_NBR, FIRST_NAME LAG_FIRST_NAME, SECOND_NAME LAG_MIDDLE_NAME, 
THIRD_NAME LAG_LAST_NAME, TAX_ID LAG_TAX_ID, PRIMARY_TEL_NBR LAG_PRIM_TEL_NBR, 
SCNDRY_TEL_NBR LAG_SCND_TEL_NBR, FAX_NBR LAG_FAX_NBR, EMAIL LAG_EMAIL
from 
    D_ENTITY
where 
    ENTY_CAT_NAME = 'Agent'



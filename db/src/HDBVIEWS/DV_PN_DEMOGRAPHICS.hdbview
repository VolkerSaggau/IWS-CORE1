

view DV_PN_DEMOGRAPHICS comment 'Person Demographics'(
DEMO_ID,
DEMO_ALL_ENTRIES_I,
INCOME_BAND,
DISP_INCOME_BAND,
AGE_BAND,
GENDER,
MARITAL_STATUS,
HIGH_VALUE_IND,
ACMDTN_CAT,
HIGH_GRD_CPLT_BAND
)
as
select 
    DEMO_ID, DEMO_ALL_ENTRIES_I, INCOME_BAND, DISP_INCOME_BAND, AGE_BAND, GENDER, 
MARITAL_STATUS, HIGH_VALUE_IND, ACMDTN_CAT, HIGH_GRD_CPLT_BAND
from 
    D_DEMOGRAPHICS
where 
    D_DEMOGRAPHICS.DEMOGRAPHICS_TYPE = 'PN'


